server {
    listen 80;
    server_name localhost;
    root /data;
    autoindex on;
    # fpm
    location ~ \.php$ {
        fastcgi_pass fpm:9000;
        fastcgi_index index.php;
        fastcgi_param SCRIPT_FILENAME $document_root$fastcgi_script_name;
        include fastcgi_params;
    }
    # fpm status
    location /fpm_status {
        access_log off;
        # allow 127.0.0.1;
        # deny all;
        fastcgi_pass fpm:9000;
        fastcgi_param SCRIPT_FILENAME $document_root$fastcgi_script_name;
        include fastcgi_params;
    }
    # nginx status
    location /ngx_status {
        stub_status on;
    }
}
server {
    listen 80 default_server;
    server_name daydaygo.top www.daydaygo.top c.daydaygo.top;
    root /data/web;
    index mygoddess.html;
}

