# pt-php-service
server {
    listen 80;
    server_name s.pt.me;
    root /var/www/innotech/pt-php-service/www;
    index index.php;
    if (!-e $request_filename) {
        rewrite ^/(.*) /index.php?$1 last;
    }
    location ~ \.php {
        fastcgi_pass fpm:9000;
        fastcgi_index index.php;
        include fastcgi.conf;
        client_max_body_size 2m;
    }
}